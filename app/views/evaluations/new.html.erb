<script type="text/javascript">
	$(function(){
		$(".start-btn").click(function(){
			$("#answer").show();
			$("#start").hide( );
		});
		$(".up-btn").click(function(){
			$("#submit").show();
			$("#answer").hide();
		});
		$(".submit").click(function(){
			$("#show").show();
			$("#submit").hide();
		})

	})	
</script>
