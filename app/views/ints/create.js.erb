<% unless @success %>
  alert("操作失败，请刷新重试");
<% else %>
  <% case params[:style_type] %>
  <% when "interview-list" %>
    <% unless @is_like %>
      $("#intInterviewSpan<%= params[:intable_id]%>").removeClass("glyphicon-star-empty").addClass("glyphicon-star");
      alert("收藏成功");
    <% else %>
      $("#intInterviewSpan<%= params[:intable_id]%>").removeClass("glyphicon-star").addClass("glyphicon-star-empty");
      alert("取消收藏成功");
    <% end %>
  <% when "interview-show" %>
    <% unless @is_like %>
      $("#collectInterview<%= params[:intable_id]%>").text("取消收藏");
      alert("收藏成功");
    <% else %>
      $("#collectInterview<%= params[:intable_id]%>").text("我要收藏");
      alert("取消收藏成功");
    <% end %>
  <% end %>
<% end %>
