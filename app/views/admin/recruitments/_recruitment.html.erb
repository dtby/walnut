<tr>
  <td><%= recruitment.name %></td>
  <td><%= recruitment.industry %></td>
  <td><%= recruitment.gender %></td>
  <td><%= recruitment.certificate %></td>
  <td><%= recruitment.number %></td>
  <td><%= recruitment.publish_time %></td>
  <td><%=recruitment.try(:degree).try(:name) %></td>
  <td><%=recruitment.try(:company).try(:name) %></td>
  <td><%= recruitment.city %></td>
  <td><%= recruitment.remuneration %></td>
  <td><%= recruitment.foreign_language %></td>
  <td><%= recruitment.others %></td>
  <td>
    <%#= link_to [:admin, specialty], class: 'btn btn-xs btn-info', title: '查看' do %>
     <!--  <span class="glyphicon glyphicon-eye-open"></span> -->
    <%#end%>
    <%= link_to edit_admin_recruitment_path(recruitment), class: 'btn btn-xs btn-default', title: '编辑' do %>
      <span class="glyphicon glyphicon-edit"></span>
    <%end%>
    <%= link_to admin_recruitment_path(recruitment), data:{ confirm: 'Are you sure?'},method: :delete, class: 'btn btn-xs btn-danger', title: '删除' do %>
      <span class="glyphicon glyphicon-trash"></span>
    <%end%>
  </td>
</tr>