<% unless @success %>
	alert("操作失败，请刷新重试");
<% else %>
	<% case params[:style_type] %>
	<% when "recruitment-list" %>
		<% unless @is_like %>
			$("#voteRecruitmentSpan<%= params[:voteable_id]%>").removeClass("glyphicon-star-empty").addClass("glyphicon-star");
			alert("收藏成功");
		<% else %>
			$("#voteRecruitmentSpan<%= params[:voteable_id]%>").removeClass("glyphicon-star").addClass("glyphicon-star-empty");
			alert("取消收藏成功");
		<% end %>
	<% when "recruitment-show" %>
		<% unless @is_like %>
			$("#collectRecruitment<%= params[:voteable_id]%>").text("取消收藏");
			alert("收藏成功");
		<% else %>
			$("#collectRecruitment<%= params[:voteable_id]%>").text("我要收藏");
			alert("取消收藏成功");
		<% end %>
	<% when "interview-show" %>
		<% unless @is_like %>
			$("#voteInterviewSpan<%= params[:voteable_id]%>").removeClass("glyphicon-star-empty").addClass("glyphicon-star");
			$("#collectInterview<%= params[:voteable_id]%>").text("取消收藏");
			alert("收藏成功");
		<% else %>
			$("#voteInterviewSpan<%= params[:voteable_id]%>").removeClass("glyphicon-star").addClass("glyphicon-star-empty");
			$("#collectInterview<%= params[:voteable_id]%>").text("我要收藏");
			alert("取消收藏成功");
		<% end %>
	<% when "comment_list" %>
		$("#comment_praise_<%= params[:voteable_id]%>").replaceWith('<span class="pull-right"><span class="glyphicon glyphicon-thumbs-up text-danger"></span> '+
			(parseInt($("#comment_praise_<%= params[:voteable_id]%>").text()) + 1)+'</span>');
	<% when "project-list"%>
		<% unless @is_like %>
			$("#projectListShowLink<%= params[:voteable_id]%>").attr("title", "取消收藏");
			$("#projectListShowLink<%= params[:voteable_id]%>").html('<%= escape_javascript(image_tag("iconfont-star.svg", width: "20px")) %>');
			alert("收藏成功");
		<% else %>
			$("#projectListShowLink<%= params[:voteable_id]%>").attr("title", "收藏");
			$("#projectListShowLink<%= params[:voteable_id]%>").html('<%= escape_javascript(image_tag("iconfont-star-empty.svg", width: "20px")) %>');
			alert("取消收藏成功");
		<% end %>
	<% when "project-pigeonhole"%>
		<% unless @is_like %>
			alert("归档成功");
		<% else %>
			alert("取消归档成功");
		<% end %>
		location.href = "<%= training_platform_root_path %>";
	<% end %>
<% end %>
